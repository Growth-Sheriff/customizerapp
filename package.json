{
  "name": "product-3d-customizer-upload",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "remix vite:dev",
    "build": "remix vite:build",
    "start": "remix-serve ./build/server/index.js",
    "typecheck": "tsc",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "test": "vitest run",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "worker:preflight": "tsx watch workers/preflight.worker.ts",
    "worker:preflight:prod": "node --import tsx workers/preflight.worker.ts",
    "worker:export": "tsx watch workers/export.worker.ts",
    "worker:export:prod": "node --import tsx workers/export.worker.ts",
    "shopify:check": "npx tsx scripts/shopify-check.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.700.0",
    "@aws-sdk/s3-request-presigner": "^3.700.0",
    "@preact/signals": "^2.5.1",
    "@prisma/client": "^5.22.0",
    "@remix-run/node": "^2.15.0",
    "@remix-run/react": "^2.15.0",
    "@remix-run/serve": "^2.15.0",
    "@sentry/remix": "^10.38.0",
    "@sentry/vite-plugin": "^4.9.0",
    "@shopify/app-bridge-react": "^4.1.3",
    "@shopify/polaris": "^13.9.0",
    "@shopify/polaris-icons": "^9.3.1",
    "@shopify/shopify-api": "^12.2.0",
    "@shopify/shopify-app-remix": "^4.1.0",
    "@shopify/shopify-app-session-storage-redis": "^5.0.4",
    "@shopify/ui-extensions": "2025.10.11",
    "bullmq": "^5.30.0",
    "ioredis": "^5.4.1",
    "isbot": "^5.1.0",
    "mime-types": "^3.0.2",
    "nanoid": "^5.0.8",
    "preact": "^10.28.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "resend": "^6.6.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.15.0",
    "@shopify/cli": "^3.88.1",
    "@shopify/theme": "^3.58.2",
    "@types/mime-types": "^3.0.1",
    "@types/node": "^22.10.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.16.0",
    "@typescript-eslint/parser": "^8.16.0",
    "eslint": "^9.15.0",
    "prisma": "^5.22.0",
    "tsx": "^4.19.0",
    "typescript": "^5.7.0",
    "vite": "^6.0.0",
    "vite-tsconfig-paths": "^5.1.0",
    "vitest": "^2.1.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
